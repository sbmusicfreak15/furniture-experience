    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
  </head>
  <body>

    <%= render "partials/navigation" %>

    <%= yield %>

    <footer>
        <p class="footer-block">&copy; <%= Date.today.year %> Bike Berlin</p>
    </footer>
  </body>
</html>
